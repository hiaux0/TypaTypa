  silent file src/my-app.html
endif
balt src/my-app.ts
setlocal fdm=expr
setlocal fde=nvim_treesitter#foldexpr()
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=99
setlocal fml=1
setlocal fdn=20
setlocal nofen
let s:l = 29 - ((16 * winheight(0) + 13) / 27)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 29
normal! 023|
wincmd w
argglobal
if bufexists(fnamemodify("src/my-app.scss", ":p")) | buffer src/my-app.scss | else | edit src/my-app.scss | endif
if &buftype ==# 'terminal'
  silent file src/my-app.scss
endif
balt src/my-app.ts
